<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Name</th>
      <th>Email</th>
      <th>Contact</th>
      <th>Comment</th>
      <th>User</th>
      <th>Team</th>
      <th>Content</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td><%= post.name %></td>
        <td><%= post.email %></td>
        <td><%= post.contact %></td>
        <td><%= post.comment %></td>
        <td><%= post.User %></td>
        <td><%= post.team %></td>
        <td><%= post.content %></td>
        <td><%= post.first_name %></td>
        <td><%= post.last_name %></td>
        <td><%= post.skill %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
